{
  "name": "@igor.dvlpr/astro-saferesource",
  "version": "1.0.0",
  "description": "🎐 Adds CSP-compliant inline scripts and styles to Astro! 🎠",
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >=21.0.0"
  },
  "files": ["dist", "index.ts", "SafeResource.astro"],
  "main": "index.ts",
  "module": "index.ts",
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "dev": "cd site && npm run dev",
    "pub": "npm run build && npm publish --access public"
  },
  "keywords": [
    "safe",
    "safe-resource",
    "csp",
    "inline",
    "hash",
    "hash-256",
    "subresource",
    "integrity",
    "script",
    "style",
    "js",
    "css",
    "astro-component",
    "withastro",
    "igorskyflyer",
    "igor.dvlpr"
  ],
  "license": "MIT",
  "author": "Igor Dimitrijević <igor.dvlpr@gmail.com> (https://github.com/igorskyflyer)",
  "funding": "https://ko-fi.com/igorskyflyer",
  "homepage": "https://github.com/igorskyflyer/npm-astro-saferesource",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorskyflyer/npm-astro-saferesource.git"
  },
  "bugs": {
    "url": "https://github.com/igorskyflyer/npm-astro-saferesource/issues"
  },
  "dependencies": {
    "@igor.dvlpr/rawelement": "^1.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.12",
    "astro": "^4.12.2"
  },
  "peerDependencies": {
    "astro": ">=1.0.0"
  }
}
